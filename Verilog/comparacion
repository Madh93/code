#! /usr/bin/vvp
:ivl_version "0.9.7 " "(v0_9_7)";
:vpi_time_precision + 0;
:vpi_module "system";
:vpi_module "v2005_math";
:vpi_module "va_math";
S_0x85f1f90 .scope module, "prueba" "prueba" 2 3;
 .timescale 0 0;
v0x85f2010_0 .var "A", 3 0;
v0x8611f20_0 .var "B", 3 0;
v0x8611f80_0 .var "C", 3 0;
    .scope S_0x85f1f90;
T_0 ;
    %movi 8, 3, 4;
    %set/v v0x85f2010_0, 8, 4;
    %end;
    .thread T_0;
    .scope S_0x85f1f90;
T_1 ;
    %movi 8, 4, 4;
    %set/v v0x8611f20_0, 8, 4;
    %end;
    .thread T_1;
    .scope S_0x85f1f90;
T_2 ;
    %load/v 12, v0x85f2010_0, 4;
    %load/v 16, v0x8611f20_0, 4;
    %cmp/u 12, 16, 4;
    %mov 12, 5, 1;
    %mov 8, 12, 1;
    %mov 9, 0, 3;
    %set/v v0x8611f80_0, 8, 4;
    %vpi_call 2 17 "$display", "dec: %d", v0x8611f80_0;
    %vpi_call 2 18 "$display", "bin: %b", v0x8611f80_0;
    %end;
    .thread T_2;
# The file index is used to find the file name in the following table.
:file_names 3;
    "N/A";
    "<interactive>";
    "comparacion.v";
